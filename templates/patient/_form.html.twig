<div class="container" style="margin-top: 20px">
{{ form_start(form) }}
<div class="form-group" style="width: 300px" >{{ form_row(form.nom, {'attr': {'class' : 'form-control','placeholder':'Nom patient.....'} } ) }}</div>
<div class="form-group" style="width: 300px" >{{ form_row(form.dateNaissance) }}</div>
<div class="form-group" style="width: 300px" >{{ form_row(form.numeroTelephone, {'attr': {'class' : 'form-control','placeholder':'Numero du medecin.....'} } ) }}</div>
<div class="form-group" style="width: 300px" >{{ form_row(form.adresse, {'attr': {'class' : 'form-control','placeholder':'Adresse du patient.....'} } ) }}</div>
<div class="form-group" style="width: 300px" >{{ form_row(form.email, {'attr': {'class' : 'form-control','placeholder':'Email du medecin.....'} } ) }}</div>
<div class="form-group" style="width: 300px" >{{ form_row(form.password, {'attr': {'class' : 'form-control','placeholder':'mot de passe du patient.....'} } ) }}</div>
<div class="form-group" style="width: 300px" >{{ form_row(form.confirme_password, {'attr': {'class' : 'form-control','placeholder':'conforme le mot de passe.....'} } ) }}</div>
    <button class="btn btn-success btn-sm">{{ button_label|default('Savegarder') }}</button>
{{ form_end(form) }}
</div> 
